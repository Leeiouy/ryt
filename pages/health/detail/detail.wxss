.poster {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 100;
  margin: auto;
  width: 542rpx;
  height: 1047rpx;
  border-radius: 20rpx;
  transform: translate3d(0, 130%, 0);
  transition: all 0.3s ease-in;
}
.show {
  transform: translate3d(0, 0, 0);
}
.poster .delt {
  position: relative;
  left: 518rpx;
  top: 28rpx;
}
.poster .delt image {
  width: 50rpx;
  height: 50rpx;
}
.poster .poster-pic {
  width: 542rpx;
  height: 850rpx;
  border-radius: 20rpx;
  background-color: #fff;
  overflow: hidden;
}
.poster-pic image {
  width: 542rpx;
  height: 850rpx;
}
.poster button {
  position: absolute;
  bottom: 5rpx;
  width: 100%;
  background-color: #fe4543;
  color: #fff;
}
.poster button::after {
  border: 0;
}
.detail-cart .borbott {
  border: 0;
}
.shelter {
  position: relative;
  width: 100%;
  height: 100%;
}
.shelter > image {
  position: absolute;
}
.shelter-play {
  position: absolute;
  z-index: 2;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.1);
}
.shelter-play > image {
  width: 65rpx;
  height: 65rpx;
}
.reply {
  position: relative;
  width: 100%;
  margin-bottom: 20rpx;
  padding: 10rpx;
  background-color: #f5f5f5;
}
.reply::after {
  position: absolute;
  top: -24rpx;
  content: '';
  display: block;
  width: 0px;
  height: 0px;
  border-bottom: 6px solid #f5f5f5;
  border-right: 6px solid transparent;
  border-top: 6px solid transparent;
  border-left: 6px solid transparent;
}
