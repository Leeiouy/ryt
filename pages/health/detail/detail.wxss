/*图文详情、发圈素材*/
.detail-tab-navbar {
  padding: 0 80rpx;
  background: #fff;
  display: flex;
  justify-content: space-around;
}
.detail-tab-navbar .item {
  height: 94rpx;
  font-size: 26rpx;
  border-bottom: 1px solid #fff;
}
.detail-tab-navbar .item.on {
  /* color:#ff5000; */
  border-bottom: 8rpx solid #ff5000;
}
.detail-tab-panel {
  background: #fff;
  overflow-x: hidden;
}
.detail-tab-content view {
  width: 100% !important;
}
.detail-tab-content image {
  width: 100% !important;
}
/* 图文详情、发圈素材为空 */
.shopcart-none {
  width: 60%;
  margin: 80rpx auto 300rpx;
}
.none-ico {
  width: 417rpx;
  height: 280rpx;
}
.none-tip {
  width: 60%;
  margin: 20rpx auto;
  text-align: center;
}
/*底部*/
.foot-none {
  height: 100rpx;
}
.detail-foot {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 100rpx;
  background: #fff;
  z-index: 9;
}
.foot-left {
  width: 40%;
}
.detail-foot .foot-left .ico-box {
  width: 40rpx;
  height: 40rpx;
}
.detail-foot .foot-left .ico-box .ico-box-num {
  position: absolute;
  left: 220rpx;
  top: -4rpx;
  padding: 0 15rpx;
  height: 30rpx;
  line-height: 30rpx;
  color: #fff;
  font-size: 22rpx;
  border-radius: 120rpx;
  background: #f96812;
}
.detail-foot .foot-right {
  height: 100%;
  color: #fff;
}
.detail-foot .foot-right .join {
  background: #f9a733;
  width: 50%;
}
.detail-foot .foot-right .buy {
  background: #f13130;
  width: 50%;
}
.detail-foot .foot-right .invalid {
  background: #d2d2d2;
}
.cart-left image {
  width: 35rpx;
  height: 35rpx;
}
.cart-left {
  width: 50%;
}
/*购物车弹窗*/
.detail-cart .box-cart-title {
  padding: 20rpx;
  display: flex;
}
.box-content {
  border-top: 1rpx solid #e5e5e5;
}
.detail-cart .box-cart-title .box-img {
  width: 190rpx;
  height: 190rpx;
  border-radius: 10rpx;
  margin-top: -60rpx;
  margin-right: 20rpx;
}
.detail-cart .box-cart-title .price-box {
  flex: 1;
  position: relative;
}
.detail-cart .box-cart-title .price-box .price {
  font-weight: bold;
  font-size: 32rpx;
  color: #ff5000;
}
.detail-cart .box-cart-title .price-box .sales {
  color: #888;
}
.detail-cart .box-cart-title .price-box .lay-close {
  position: absolute;
  right: 0;
  top: 0;
  width: 28rpx;
  height: 28rpx;
}
.detail-cart .box-spec {
  padding: 20rpx;
  border-bottom: 1rpx solid #e5e5e5;
}
.detail-cart .box-spec .spec-items {
  display: flex;
  flex-wrap: wrap;
  padding-right: -40rpx;
  margin-top: 12rpx;
}
.detail-cart .box-spec .spec-items button {
  margin-left: 0;
}
.detail-cart .box-spec .spec-items button::after {
  border: 0;
}
.detail-cart .box-spec .spec-item {
  height: 50rpx;
  line-height: 46rpx;
  padding: 0 20rpx;
  font-size: 26rpx;
  margin-right: 20rpx;
  margin-bottom: 20rpx;
  border: 2rpx solid #e5e5e5;
  border-radius: 6rpx;
  background-color: #fff;
  color: #000;
}
.detail-cart .box-spec .spec-item.on {
  color: #ff5000;
  border-color: #ff5000;
}
.detail-cart .box-spec .spec-items button.on {
  color: #ff5000;
  border-color: #ff5000;
}
.detail-cart .box-spec .spec-item.grey {
  background: #f1f1f1;
}
.updown-box {
  padding: 20rpx;
  /* border-top:1rpx solid #e5e5e5; */
  border-bottom: 1rpx solid #e5e5e5;
}
/*数量加减*/
.updown {
  border: 1px solid #e5e5e5;
  border-radius: 6rpx;
  overflow: hidden;
  height: 70rpx;
}
.updown image {
  width: 28rpx;
  height: 28rpx;
}
.updown .up,
.updown .down {
  width: 70rpx;
