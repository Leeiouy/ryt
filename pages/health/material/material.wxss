page {
  background-color: #f5f5f5;
}

.top {
  display: flex;
  justify-content: space-between;
  align-items: center;
  background-color: #fff;
  margin: 20rpx 30rpx;
  border-radius: 10rpx;
  padding: 20rpx 22rpx;
}

.good-title {
  font-size: 30rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  margin-right: 200rpx;
}

.good-img {
  width: 120rpx;
  height: 120rpx;
  margin-right: 20rpx;
}

.middle {
  background-color: #fff;
  padding: 41rpx 32rpx 0;
  margin: 0 30rpx;
}

.photos {
  display: flex;
  flex-wrap: wrap;
  padding-bottom: 30rpx;
}

.img {
  width: 30%;
  height: 200rpx;
  background-color: #f5f5f5;
  margin: 14rpx 10rpx;
}

.record {
  padding: 29rpx 30rpx 0;
  background-color: #fff;
  margin: 0 30rpx 42rpx;
}

.record-nums {
  color: #fc4902;
}

.item {
  padding: 30rpx 0;
  border-bottom: 1rpx solid #e0e0e0;
}

.head {
  width: 64rpx;
  height: 64rpx;
  border-radius: 50%;
  margin-right: 10rpx;
}

.bot-opration {
  position: fixed;
  bottom: 0rpx;
  width: 100%;
  background-color: #fff;
  padding: 12rpx 30rpx;
}

.opration {
  width: 320rpx;
  height: 76rpx;
  border-radius: 5rpx;
  border: 1rpx solid rgba(224, 224, 224, 1);
}

.load-opration {
  width: 320rpx;
  height: 76rpx;
  color: #fff;
  background: rgba(228, 89, 42, 1);
  border-radius: 5rpx;
}

.copy {
  width: 30rpx;
  height: 32rpx;
}

.download {
  width: 32rpx;
  height: 28rpx;
}
