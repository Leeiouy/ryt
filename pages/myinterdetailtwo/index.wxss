/* pages/myinterdetailtwo/index.wxss */

page {
  background-color: #f5f5f5;
}

/*收藏商品*/

.collect-item {
  padding: 28rpx;
  background: #fff;
  border-bottom: 1px solid #e5e5e5;
}

.collect-item .img-box {
  width: 190rpx;
  height: 190rpx;
  margin-right: 20rpx;
}

.collect-item .twoline {
  line-height: 40rpx;
  font-size: 30rpx;
  margin-bottom: 20rpx;
}

.collect-item .item-price {
  margin-top: 20rpx;
}

.collect-item .item-price .cor-red {
  font-size: 36rpx;
  margin-right: 20rpx;
}

.collect-item .item-price .cor-grey {
  text-decoration: line-through;
}

.collect-item .item-delete {
  width: 32rpx;
  height: 32rpx;
}

.weui-loadmore__tips_in-line {
  background: #f1f1f1;
}

.yellow{
  color: #FD7040;
}