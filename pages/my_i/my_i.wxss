.my {
  position: relative;
}
.my .bg {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  z-index: -1;
  height: 700rpx;
  width: 100%;
}
.my .user {
  padding-top: 100rpx;
  color: white;
}
.my .user .user_avatar {
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
  background-color: white;
}
.my .user .user_name {
  font-size: 32rpx;
}
.my .module {
  width: 90%;
  margin: 0 auto;
  padding: 20rpx;
  background-color: white;
  border-radius: 30rpx;
  margin-top: 24rpx;
}
.my .module .title {
  border-bottom: 1rpx solid #f2f2f2;
  padding: 10rpx 0;
}
.my .module .order {
  margin-top: 30rpx;
  text-align: center;
  font-size: 26rpx;
  color: #666666;
}
.my .module .order > view {
  flex: 1;
}
.my .module .order > view:active {
  opacity: 0.7;
}
.my .module .order image {
  width: 60rpx;
}
.my .options {
  margin: 30rpx auto;
  border-radius: 30rpx;
  background-color: white;
  padding: 0 30rpx;
  width: 90%;
}
.my .options .options_item {
  padding: 0 20rpx;
  height: 100rpx;
}
.my .options .options_item .flex1 {
  margin: 0 20rpx;
}
.my .options .options_item:active {
  opacity: 0.7;
}
.my .options .bb {
  border-bottom: 1rpx solid #e6e6e6;
}
