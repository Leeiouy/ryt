.nav {
  width: 690rpx;
  min-height: 250rpx;
  border-radius: 20rpx;
  background: #fff;
  margin: 20rpx auto 0;
  box-shadow:1rpx 7rpx 14rpx 0rpx rgba(196,196,196,0.32);
}

.nav .title {
  height: 84rpx;
  width: 100%;
  padding: 0 26rpx;
  line-height: 84rpx;
  font-size: 28rpx;
  color: #343434;
  border-bottom: 1px solid #E1E1E1;
}

.nav .title.health {
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.nav .title.health .level-h {
  height: 30rpx;
  width: 100rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  position: relative;
}

.nav .title.health .level-h image {
  width: 100%;
  height: 100%;
}

.nav .title.health .level-h text {
  color: #fff;
  font-size: 20rpx;
  font-weight: 500;
  position: absolute;
  left: 40%;
}



.nav .list {
  width: 100%;
  /* padding: 0 30rpx; */
  padding-top: 15rpx;
  display: flex;
  align-items: center;
  justify-content: flex-start;
  flex-wrap: wrap;
}

.nav .list .item {
  width: 25%;
  height: 150rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.nav .list .item image {
  height: 60rpx;
  width: 60rpx;
