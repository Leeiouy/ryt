/*为空提示*/
@import '../template/empty.wxss';

page {
  background-color: #f5f5f5;
}


.preventTouchMove{
    top: 0px ; 
    left:   0px ; 
    width:   100% ; 
    height:   100% ; 
    overflow:   hidden ; 
    position:   fixed ; 
    z-index:   0 ; 
}

.top {
  width: 100%;
  background-color: #fff;
  padding-left: 49rpx;
  position: fixed;
  top: 0rpx;
  z-index: 10000000;
}

.tabs {
}

.tab {
  font-size: 31rpx;
  color: #333;
  font-weight: 550;
  margin-right: 65rpx;
}

.active {
  display: flex;
  flex-direction: column;
  color: #e4592a;
}

.active:after {
  content: "";
  width: 92rpx;
  height: 6rpx;
  background-color: #e4592a;
  border-radius: 10rpx;
}

.search-box {
  margin-top: 170rpx;
}

.seach-time {
  width: 100%;
  padding: 20rpx 23rpx;
  position: relative;
  z-index: 1000;
  background-color: #fff;
}

.time {
  width: 170rpx;
  height: 60rpx;
  background: #ebebeb;
  border-radius: 5rpx;
  margin-right: 10rpx;
  text-align: center;
  line-height: 60rpx;
  color: #999;
}

.c-today {
  background-size: 100% 100%;
  color: rgba(228, 89, 42, 1);
  /* background-color: #e4592a; */
  /* color: #fff; */
  background-image: url('data:image/png;base64,');
}

.search-ico {
  width: 30rpx;
  height: 32rpx;
}

.search-init {
  -webkit-transform: translateY(-100%);
  transform: translateY(-100%);
}

.search {
  background-color: #fff;
  display: flex;
}

.empty-imgbox {
  width: 495rpx;
  margin: 150rpx auto 0;
  display: block;
}

.search-bg {
  width: 94%;
  margin: 20rpx auto;
  background-color: #f8f8f8;
  padding: 20rpx;
  border-radius: 10rpx;
}

input {
  width: 70%;
  margin-left: 15rpx;
}

.wrap {
  padding-bottom: 170rpx;
}

.total {
  width: 100%;
  height: 60rpx;
  background: rgba(251, 225, 147, 1);
  color: #93440a;
  text-align: center;
  line-height: 60rpx;
}

.item {
  padding: 32rpx 30rpx;
  background-color: #fff;
  border-bottom: 1rpx solid #e0e0e0;
}
.item:last-child{
  border-bottom: none;
}
.middle {
  margin-left: 28rpx;
  width: 80%;
}

.dis-ico {
  width: 100rpx;
  height: 30rpx;
  border-radius: 5rpx;
}

.dis-name {
  margin: 18rpx 0;
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
}

.mid-bottom {
  position: relative;
  width: 485rpx;
}

.mid-bottom text {
  font-size: 25rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
}
.middle .goods-freight {
  font-size: 24rpx;
  line-height: 120%;
  color: rgb(255, 68, 68);
}

.mid-bottom text:nth-child(1) {
  display: inline;
  text-overflow: ellipsis;
}

.mid-bottom text:nth-child(2) {
  position: absolute;
  right:9rpx;
}

.right {
  width: 30%;
  display: flex;
  flex-direction: column;
  align-items: flex-end;
}

.price {
  font-size: 28rpx;
  color: rgba(228, 89, 42, 1);
}

.item-delete {
  width: 32rpx;
  height: 33rpx;
  margin-top: 80rpx;
}

/*底部结算*/
.seat-bottom{
  height: 100rpx;
}
.shopcart-foot {
  position: fixed;
  z-index: 100;
  /* bottom:130rpx; */
  width: 100%;
  height: 100rpx;
  background: #fff;
  /* margin-bottom: 104rpx; */
}
.shopcart-foot .foot-select {
  width: 100rpx;
  height: 100%;
}

.shopcart-foot .foot-all, .shopcart-foot .foot-total {
  /* line-height: 100rpx; */
}

.foot-all text {
  font-size: 30rpx;
  font-family: PingFang;
  font-weight: 500;
}

.shopcart-foot .foot-total {
  /* width: 200rpx; */
  text-align: right;
  padding-right: 20rpx;
  margin-left: 50rpx;
  color: #4e4e4e;
  width: 45%;
  /* padding-left: 14rpx; */
}

.shopcart-foot .foot-total text{
  text-align: left;
  width: 140rpx;
  height: 60rpx;
}

.price {
  width: 100rpx;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(228, 89, 42, 1);
}

.done-orders {
  width: 150rpx;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
}

.shopcart-foot .foot-sub {
  width: 180rpx;
  height: 65%;
  background: rgba(228, 89, 42, 1);
  color: #fff;
  border-radius: 30rpx;
  margin: 19rpx 10rpx 15rpx 0;
}

/* 确认交单弹出框 */
.dialog-content{
  display: flex;
  justify-content: center;
}
.dialog {
  display: flex;
  flex-direction: column;
  background-color: #fff;
  position: absolute;
  z-index: 99999;
  top: 200rpx;
  border-radius: 10rpx;
}

.my-buy {
  width: 486rpx;
  height: 85rpx;
  margin: 0 auto;
  margin-top: -54rpx;
}

.dia-content {
  background-color: #fff;
  border-radius: 20rpx;
  padding: 0 30rpx 20rpx 30rpx;
  width: 530rpx;
  height:580rpx;
}

.content-title {
  font-size: 35rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  margin: 10rpx auto;
}

.subtitle {
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  margin-bottom: 25rpx;
}

.weui-demo-dialog {
  visibility: hidden;
  opacity: 0;
  transition: opacity 0.3s;
}

.weui-demo-dialog_show {
  visibility: visible;
  opacity: 1;
}

.text-bg {
  width: 100%;
  height: 140rpx;
  background: rgba(245, 245, 245, 1);
  border-radius: 5rpx;
  margin-top: 15rpx;
  padding: 15rpx;
}

textarea {
  width: 100%;
  height: 100%;
}

.text-line {
  margin-left: 10rpx;
}

.text-linetwo {
  padding-left: 10rpx;
  margin-top: 20rpx;
}

.text-line text:nth-child(1) {
  font-size: 26rpx;
  color: #333;
}

.text-line text:nth-child(2) {
  font-size: 30rpx;
  color: #fc4902;
}

.text-line text:nth-child(3) {
  font-size: 26rpx;
  color: #333;
}

.text-linetwo image {
  width: 28rpx;
  height: 28rpx;
}

.text-linetwo text:nth-child(2) {
  font-size: 26rpx;
  color: #333;
  margin-left: 7rpx;
}

.text-linetwo text:nth-child(3) {
  font-size: 20rpx;
  color: #fc4902;
}

.btns {
  width: 100%;
  display: flex;
  justify-content: space-between;
  margin-top: 35rpx;
}

.cancel {
  background: rgba(255, 255, 255, 1);
  border-radius: 5rpx;
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  width: 48%;
  height: 80rpx;
  line-height: 80rpx;
}

.sure {
  background: rgba(228, 89, 42, 1);
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  width: 48%;
  height:80rpx;
  line-height: 80rpx;
}

button {
  margin-left: 2rpx;
  margin-right: 0rpx;
}

/* 确认交单弹出框end */

/* 待审核 */

.wait-item {
  padding: 19rpx 30rpx;
  background-color: #fff;
  margin: 18rpx 20rpx 0;
}

.info-right {
  margin-left: 17rpx;
}

.info-right image {
  width: 88rpx;
  height: 26rpx;
  background: rgba(191, 158, 113, 1);
  border-radius: 6rpx;
}

.head {
  width: 96rpx;
  height: 96rpx;
  background: rgba(252, 73, 2, 1);
  border-radius: 50%;
}

.phone {
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  margin-right: 13rpx;
}

.wait-time {
  font-size: 24rpx;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
}

.wait-total {
  padding: 42rpx;
  margin: 26rpx 0;
  background-color: #f6f6f6;
  display: flex;
  justify-content: space-between;
}

.wait-total text:nth-child(1) {
  font-size: 28rpx;
  color: #999;
}

.wait-total text:nth-child(2) {
  font-size: 18rpx;
  color: #fc4902;
}

.wait-total text:nth-child(3) {
  font-size: 32rpx;
  color: #fc4902;
}

.arrow {
  width: 14rpx;
  height: 28rpx;
}

.wait-nums text:nth-child(2) {
  font-size: 28rpx;
  color: #fc4902;
}

.wait-nums text:nth-child(3) {
  font-size: 22rpx;
  color: #999;
}

.change {
  color: #333;
}

.ps text, .wait-price text, .change text {
  color: #999;
}

