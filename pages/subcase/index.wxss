.wrap-x-one{
  margin-bottom: 500rpx;
}
.wrap-x{
  margin-bottom: 400rpx;
}
.search-time{
  white-space: nowrap;
  color: #fc4902;
  margin-left: 20rpx;
}
.down{
  width: 17rpx;
  height: 10rpx;
  margin-left: 11rpx;
}


.tabbar {
  width: 100%;
  height: 88rpx;
  display: flex;
  align-items: center;
  justify-content: space-around;
  top: 54rpx;
  position: fixed;
  z-index: 99;
}

.tabbar .item {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
  height: 100%;
  border: 0;
  padding: 0;
  position: relative;
}

.tabbar .item.on {
  color: #E45A2A;
}

.tabbar .item .block {
  height: 6rpx;
  width: 80rpx;
  background: #fff;
  border-radius: 3rpx;
  position: absolute;
  bottom: 10rpx;
}

.tabbar .item.on .block {
  background: #E45A2A;
}