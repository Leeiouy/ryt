/* pages/chooserankcard/index.wxss */

page {
  background-color: #f5f5f5;
}

.rankcards {
  padding: 26rpx 25rpx;
  margin-top: 20rpx;
  background-color: #fff;
}

.rankcards text:nth-child(1) {
  font-size: 30rpx;
  color: #2c2c2c;
  font-weight: bold;
}

.add-card {
  position: fixed;
  bottom: 0rpx;
  width: 100%;
  height: 88rpx;
  background: rgba(253, 112, 64, 1);
  font-size: 32rpx;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  text-align: center;
  line-height: 88rpx;
}

