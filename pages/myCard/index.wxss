}
.card-list{
  margin-bottom: 118rpx;
}
.rankcards{
  padding: 26rpx 25rpx;
  margin-top: 20rpx;
  background-color: #fff;
}
.rankcards text:nth-child(1){
  font-size: 30rpx;
  color: #2C2C2C;
}
.btn{
  width:90rpx;
height:48rpx;
background:rgba(237,237,237,1);
