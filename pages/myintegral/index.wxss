/* pages/integral/index.wxss */

page {
  background-color: #f5f5f5;
}

.info {
  width: 100%;
  height: 520rpx;
  position: relative;
}
.useful{
  position: absolute;
  top: 0rpx;
  margin: 44rpx 0 0 285rpx;
  color: #fff;
  width: 189rpx;
  height: 206rpx;
  background-size: 100%;
  background-image: url('data:image/png;base64,')
}
.useful text:nth-child(1){
  font-size: 32rpx;
}
.useful text:nth-child(2){
  font-size: 22rpx;
}
.withdrawl{
  position: absolute;
  right: 0rpx;
  top: 0rpx;
  margin: 44rpx 32rpx 0 0;
  width:120rpx;
height:50rpx;
background:rgba(255,112,66,1);
border:1rpx solid rgba(255,255,255,1);
border-radius:25rpx;
font-size:22rpx;
text-align: center;
line-height: 50rpx;
font-weight:400;
color:rgba(255,255,255,1);
}

.inter-detail{
  position: absolute;
  display: flex;
  justify-content: space-around;
  bottom: 115rpx;
  color: #fff;
  width: 100%;
}
.inter text:nth-child(1){
  font-size: 28rpx;
}
.inter text:nth-child(2){
  font-size: 32rpx;
}

.achieves{
  position: absolute;
  top:403rpx;
  width: 100%;
}
.day-achieve{
  width: 93%;
  background-color: #fff;
  margin: 0 29rpx;
background:rgba(255,255,255,1);
box-shadow:0px 2rpx 5rpx 0rpx rgba(0, 0, 0, 0.06);
border-radius:20rpx;
margin-top: 20rpx;
}
.day-top{
  padding: 28rpx;
  border-bottom: 1rpx solid #E0E0E0
}
.time text{
  color: #FD7040;
  margin-right: 11rpx;
}
.down{
  width:17rpx;
height:10rpx;
}
.content{
  display: flex;
  justify-content: space-around;
  padding: 42rpx 40rpx;
}
.personal{
font-size:28rpx;
font-weight:400;
color:rgba(51,51,51,1);
}

.person-achie text{
font-size:32rpx;
font-weight:500;
}

.team-achie text{
  color: #333333;
  font-size: 32rpx
}
.team-achie text:nth-child(1){
  margin-right: 30rpx;
}
.btn{
  width:690rpx;
height:80rpx;
background:rgba(253,112,64,1);
border-radius:10rpx;
font-size:32rpx;
font-weight:400;
color:rgba(255,255,255,1);
line-height:80rpx;
text-align: center;
margin: 648rpx auto 72rpx;
}