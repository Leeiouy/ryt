/* pages/setting/index.wxss */

page {
  background-color: #f5f5f5;
}

.btn {
  background-color: #fff;
  color: #e4592a;
  width: 100%;
  height: 95rpx;
  line-height: 95rpx;
  text-align: center;
  margin-top: 30rpx;
  font-size: 30rpx;
}
.weui-cell{
  border-bottom: 1rpx solid rgba(0, 0, 0, 0.1);
}

.weui-cell:last-child{
  border-bottom: none;
}
.weui-cell:before {
  border-top: none;
}
.weui-cells:after {
  border: none;
}
.weui-panel:after{
  border-bottom: none;
}